{
  "name": "eslint-config-quri",
  "version": "5.0.0",
  "description": "Quri's ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "prelint": "editorconfig-tools check * rules/* test/*",
    "lint": "eslint .",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "prepublish": "(in-install || eslint-find-rules --unused) && (not-in-publish || npm test) && safe-publish-latest",
    "pretest": "npm run --silent lint",
    "test": "npm run --silent tests-only",
    "travis": "cd ../eslint-config-quri-base && npm install && npm link && cd - && npm link eslint-config-quri-base && npm run --silent test ; npm unlink eslint-config-quri-base >/dev/null &"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quri/javascript"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "quri",
    "javascript",
    "styleguide"
  ],
  "author": "Jake Teton-Landis (https://twitter.com/@jitl)",
  "contributors": [
    {
      "name": "Jake Teton-Landis",
      "url": "https://twitter.com/jitl"
    },
    {
      "name": "Jordan Harband",
      "email": "ljharb@gmail.com",
      "url": "http://ljharb.codes"
    },
    {
      "name": "Harrison Shoff",
      "url": "https://twitter.com/hshoff"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quri/javascript/issues"
  },
  "homepage": "https://github.com/quri/javascript",
  "dependencies": {
    "eslint-config-quri-base": "^4.0.0"
  },
  "devDependencies": {
    "babel-tape-runner": "^2.0.1",
    "editorconfig-tools": "^0.1.1",
    "eslint": "^3.19.0",
    "eslint-find-rules": "^1.14.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "in-publish": "^2.0.0",
    "react": ">= 0.13.0",
    "safe-publish-latest": "^1.1.1",
    "tape": "^4.6.3"
  },
  "peerDependencies": {
    "eslint": "^3.19.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3"
  },
  "engines": {
    "node": ">= 4"
  }
}
